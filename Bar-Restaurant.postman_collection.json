	"item": [
		{
			"name": "Gateway Service",
			"description": "API Gateway - Main entry point for all requests",
			"item": [
				{
					"name": "Health Check",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{gateway_url}}/api/v1/gateway/p/health",
							"host": ["{{gateway_url}}"],
							"path": ["api", "v1", "gateway", "p", "health"]
						},
						"description": "Check gateway health and all dependent services"
					}
				}
			]
		},
		{
			"name": "Session Service",
			"description": "Authentication and session management",
			"item": [
				{
					"name": "Health Check",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{gateway_url}}/api/v1/sessions/p/health",
							"host": ["{{gateway_url}}"],
							"path": ["api", "v1", "sessions", "p", "health"]
						}
					}
				},
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"if (jsonData.data && jsonData.data.token) {",
									"    pm.collectionVariables.set('token', jsonData.data.token);",
									"    console.log('Token saved:', jsonData.data.token);",
									"} else {",
									"    console.warn('No token found in response');",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"admin\",\n    \"password\": \"admin\"\n}"
						},
						"url": {
							"raw": "{{gateway_url}}/api/v1/sessions/p/login",
							"host": ["{{gateway_url}}"],
							"path": ["api", "v1", "sessions", "p", "login"]
						},
						"description": "Authenticate user and get session token"
					}
				},
				{
					"name": "Validate Session",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"token\": \"{{token}}\"\n}"
						},
						"url": {
							"raw": "{{gateway_url}}/api/v1/sessions/p/validate",
							"host": ["{{gateway_url}}"],
							"path": ["api", "v1", "sessions", "p", "validate"]
						},
						"description": "Validate an existing session token"
					}
				},
				{
					"name": "Logout",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"token\": \"{{token}}\"\n}"
						},
						"url": {
							"raw": "{{gateway_url}}/api/v1/sessions/logout",
							"host": ["{{gateway_url}}"],
							"path": ["api", "v1", "sessions", "logout"]
						},
						"description": "Logout and invalidate session (requires authentication)"
					}
				}
			]
		},
